+ Response 200 (application/json; charset=utf-8)
    
    + Body
    
            {
                "data": {
                    "createCard": {
                        "card": {
                            "id": "32",
                            "title": "Charlie",
                            "due_date": "2017-01-15T09:05:06-02:00",
                            "assignees":[
                                {
                                    "id": "7",
                                    "username": "beth"
                                },
                                {
                                    "id": "8",
                                    "username": "mary"
                                }
                            ],
                            "fields":[
